{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "jsx": "react",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true
  }
}
