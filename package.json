{
  "name": "500",
  "version": "1.0.0",
  "description": "500 card game",
  "main": "lib/app.js",
  "author": "Kirby",
  "license": "MIT",
  "scripts": {
    "babel": "babel src --config-file ./babel/node.js --no-babelrc --out-dir lib --ignore \"src/ui,src/index.js\"",
    "build": "run-s clean && cross-env NODE_ENV=production run-p babel webpack:prod",
    "build:local": "run-s clean webpack:dll",
    "clean": "rm -rf ./lib ./assets/bundle ./assets/json",
    "start": "node ./lib/server/server.js",
    "start:local": "cross-env NODE_ENV=localhost babel-node --no-babelrc --config-file ./babel/node.js ./src/server/server.js",
    "test": "cross-env NODE_ENV=test jest",
    "webpack:dll": "webpack --config ./webpack/config/dll.js",
    "webpack:prod": "webpack --config ./webpack/config/app.js",
    "verify": "echo 'tests and eslint not setup yet'"
  },
  "dependencies": {
    "express": "^4.16.4",
    "ioredis": "^4.14.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^4.7.1",
    "spdy": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "assets-webpack-plugin": "^3.9.7",
    "autoprefixer": "^8.6.5",
    "babel-eslint": "^8.2.6",
    "babel-jest": "24.9.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "3.10.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "24.9.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^2.1.6",
    "react-test-renderer": "16.11.0",
    "sass-loader": "^6.0.7",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-dev-server": "^3.2.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
