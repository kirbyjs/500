  "scripts": {
    "babel:lambdas": "babel src -d lib --ignore 'src/*.','src/ui/' -x '.ts','.js' --config-file ./babel/node.js --no-babelrc ",
    "build": "run-s clean && cross-env NODE_ENV=production run-p babel:lambdas webpack:prod",
    "build:lambdas": "run-s clean && npm run babel:lambdas",
    "clean": "rm -rf ./lib ./assets/bundle",
    "coverage": "npm run test -- --collect-coverage",
    "eslint": "eslint .",
    "lint": "npm run eslint && npm run stylelint",
    "start": "npm run webpack:dev",
    "start:apigateway-ws": "nodemon scripts/local/start-api-gateway-websocket.ts",
    "test": "cross-env NODE_ENV=test jest",
    "tsc": "tsc",
    "stylelint": "stylelint ./sass",
    "verify": "run-p lint coverage tsc",
    "webpack:dev": "webpack-dev-server --config webpack/config/dev-server.ts --hot",
    "webpack:prod": "webpack --config webpack/config/app.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.615.0",
    "config": "^3.2.5",
